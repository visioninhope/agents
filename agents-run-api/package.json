    "@types/jmespath": "^0.15.2",
    "@types/node": "^20.11.24",
    "@types/traverse": "^0.6.37",
    "@vitest/coverage-v8": "^2.0.0",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
