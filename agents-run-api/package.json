    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "ai": "5.0.11",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "destr": "^2.0.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "exit-hook": "^4.0.0",
    "fetch-to-node": "^2.1.0",
    "hono": "^4.9.7",
    "jmespath": "^0.16.0",
    "keytar": "^7.9.0",
    "nanoid": "^5.1.5",
    "traverse": "^0.6.11",
    "ts-pattern": "^5.7.1",
    "zod": "^4.1.5"
